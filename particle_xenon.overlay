/ {
	leds {
		compatible = "gpio-leds";
		led0: led_0 {
		  gpios = <&gpio1 13 0>;
		  label = "STATUS_LED";
	  	};
	};

	gpio_keys {
		compatible = "gpio-keys";
		lv_io_in: button_0 {
			gpios = <&gpio0 26 GPIO_PUD_NORMAL>;
			label = "LV_IO_IN";
		};
	};

	/* These aliases are provided for compatibility with samples */
	aliases {
		led0 = &led0;
	};
};

&pwm0 {
	status = "okay";
	ch0-pin = <45>;	// status LED
	ch0-inverted;
	ch1-pin = <8>;	// power out
	ch1-inverted;
};
